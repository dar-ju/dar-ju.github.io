{
  "name": "node-template",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "main": "server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .js",
    "format": "prettier . --write",
    "db:create": "node db-create.js",
    "db:migrate": "knex migrate:latest",
    "dev": "cross-env npm run db:create && npm run db:migrate && nodemon server.js",
    "start": "cross-env npm run build && npm run db:create && npm run db:migrate && node server.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "lint-staged": "^15.5.2",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.6.1",
    "cookie-parser": "^1.4.7",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "knex": "^3.1.0",
    "mysql2": "^3.14.1",
    "nanoid": "^5.1.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  }
}
